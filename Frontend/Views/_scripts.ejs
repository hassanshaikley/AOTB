<script>
	<% if (name) { %>
		var localPlayerName = "<%= name %>";
		var characterType = "<%= character %>";
	<% } else {%>
		var a = Math.floor(Math.random()*3);
		if ( a == 0){
			var characterType = "Fly";
		} else if (a == 1 ){
			var characterType = "Redhatter";
		} else if (a == 2) {
			var characterType = "Shanker";
		} else {
			var characterType = "Grimes";
		}
		var localPlayerName = "unknown";

 	<% } %>
	if (location.origin != "http://localhost:5000"){
  		$('body').bind('contextmenu', function(){ return false; });
	}
	PORT = 5000;


</script>
